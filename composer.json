{
    "name": "majal/ddd",
    "description": "DDD Laravel-structure-generator",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Mohamed Emad",
            "email": "mohamed,mohamed65108@gmail.com"
        }
    ],
    "require": {
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-query-builder": "^3.3",
        "livewire/livewire": "^2.2",
        "laravel/ui": "^3.0",
        "mediconesystems/livewire-datatables": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Majal\\DDD\\":"src/",
            "App\\":"app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Majal\\DDD\\":"src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel":{
            "providers":[
                "Majal\\DDD\\DomainDriverDesignServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    }
}
